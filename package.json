  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "browser-sync": "2.18.7",
    "clean-css": "3.4.24",
    "globby": "6.1.0",
    "imagemin": "5.1.2",
    "imagemin-mozjpeg": "6.0.0",
    "imagemin-pngquant": "5.0.0",
    "imagemin-svgo": "5.2.0",
    "node-version-assets": "1.2.0",
    "postcss-cli": "2.6.0",
    "postcss-color-function": "3.0.0",
    "postcss-hexrgba": "0.2.1",
    "postcss-import": "7.1.3",
    "postcss-math": "0.0.7",
    "postcss-mixins": "5.4.1",
    "postcss-nested": "1.0.0",
    "postcss-simple-vars": "3.0.0",
    "stylelint": "7.7.1",
    "stylelint-trendyminds-standard": "1.0.6",
    "svg-sprite": "1.3.6",
    "webpack": "2.2.1",
    "xo": "0.17.1"
  },
  "dependencies": {
    "jquery": "3.1.1",
    "normalize.css": "5.0.0",
    "picturefill": "3.0.2",
    "svg-injector": "1.1.3"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "xo": {
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "new-cap": [
        "error",
        {
          "properties": false
        }
      ],
      "no-new": 0,
      "unicorn/filename-case": 0,
      "unicorn/explicit-length-check": 0,
      "import/no-unassigned-import": 0,
      "no-alert": 0
    }
  },
  "stylelint": {
    "ignoreFiles": "./app/assets/styles/base/_sprite.css",
    "extends": "stylelint-trendyminds-standard"
  }
